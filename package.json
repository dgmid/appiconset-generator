{
  "name": "appiconset-generator",
  "version": "1.2.0",
  "productName": "Appiconset Generator",
  "description": "A utility app for generating Apple appiconsets.",
  "main": "dist/js/main.min.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "./node_modules/.bin/electron-rebuild",
    "package": "npm prune -production; electron-packager . --electron-version=4.1.3 --overwrite --platform=darwin --arch=x64 --icon=dist/assets/icon/icon.icns --ignore=\"app-source|gulpfile\\.js|README\\.md\" --prune=false --out=build --darwinDarkModeSupport=true"
  },
  "author": {
    "name": "D.G Midwinter",
    "email": "duncan@midwinter-dg.com",
    "url": "https://www.midwinter-dg.com"
  },
  "homepage": "https://www.midwinter-dg.com/mac-apps/appiconset-generator.html",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dgmid/appiconset-generator"
  },
  "dependencies": {
    "electron-sharp": "^0.20.8",
    "electron-store": "^5.1.1",
    "fs-extra": "^9.0.0",
    "i18next": "^19.3.4",
    "i18next-electron-language-detector": "0.0.10",
    "i18next-sync-fs-backend": "^1.1.1",
    "jquery": "^3.5.0",
    "jquery-i18next": "^1.2.1",
    "load-json-file": "^6.2.0"
  },
  "devDependencies": {
    "electron": "^18.3.7",
    "electron-rebuild": "^1.10.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-htmlmin": "^5.0.1",
    "gulp-iconutil": "^1.2.2",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify-es": "^2.0.0",
    "pump": "^3.0.0",
    "reset-css": "^5.0.1"
  }
}
